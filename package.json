{
  "name": "boilerplate-webpack",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_ENV=local && yarn create-app-config-file && webpack-dev-server --config ./webpack/configuration/development.configuration.babel.js",
    "dev": "export NODE_ENV=development && yarn create-app-config-file && webpack-dev-server --config ./webpack/configuration/development.configuration.babel.js",
    "prod": "export NODE_ENV=production && yarn create-app-config-file && webpack --config ./webpack/configuration/production.configuration.babel.js",
    "serve-production": "live-server --entry-file=dist/index.html --port=5000",
    "create-app-config-file": "babel-node scripts/create-app-config-file.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "check-prop-types": "^1.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.3.0",
    "jest-enzyme": "^7.1.2",
    "live-server": "^1.2.1",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.20",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "styled-components": "^5.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
